  "sentences": [
    {
      "text": {
        "content": "Rocky was living in an empty house in a housing scheme.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "Unfortunately the house was recently bought over and subsequently the new owner did not want him there anymore.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "Ever since Rocky is looking for a new home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "He is an excellent guard dog who needs not much.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "Only shelter from the rain and sun, food and a friendly owner.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "That's all.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
