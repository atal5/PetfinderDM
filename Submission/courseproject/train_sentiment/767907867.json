  "sentences": [
    {
      "text": {
        "content": "Maxine was 4 months old when I rescued her from the streets.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "She had ear mites and skin infection but she has since been cured of these.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "Today, she is a healthy and very lovable little girl and current in foster care at my friend's place.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "She is a gentle, fun and loving little girl and will bring lots of fun to family with young children.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    }
