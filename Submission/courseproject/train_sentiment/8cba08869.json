{
  "sentences": [
    {
      "text": {
        "content": "Chessie is a lovely calico cat rescued by my friend who volunteers at the Cheshire Home, Selayang.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "Chessie was pregnant when found and has since given birth to 2 lovely kittens.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "She has been treated for, and cured of, feline mange, and has been vaccinated and dewormed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "She has already been spayed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Won't you please open up your heart and home to little Chessie?",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    }
  ],
