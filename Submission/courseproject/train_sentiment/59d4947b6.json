  "sentences": [
    {
      "text": {
        "content": "Friendly &amp; active type of Shih Tzu dog is urgently need to relocate to new owner due to current owner is getting elderly &amp; not able to take care of her.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "This dog is currently in Ipoh but arrangement can be made if anyone from KL or Ipoh is interested to adopt her.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "Prefer someone who is animal lover &amp; can provide long term care to her.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "Thank you so much.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    }
