  "sentences": [
    {
      "text": {
        "content": "Tasha is a shy puppy that loves cuddles.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "She is a good girl and can be easily trained with the right amount of patience.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "Adopter should treat Tasha as a part of their family and cannot tie her up or cage her for long periods of time.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "She must also be spayed at 5 months of age.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Only very interested adopters will be considered.",
        "beginOffset": -1
