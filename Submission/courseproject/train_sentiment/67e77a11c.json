  "sentences": [
    {
      "text": {
        "content": "We adopted Foxy from Paw's about 2 years ago.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "It's great to see how much she has grown and flourished in her new environment.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Unfortunately, we're short handed at this moment and are not able to care for her the way we used to.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    },
    {
      "text": {
        "content": "It is our hope that she wll go to a good home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    }
