{
  "sentences": [
    {
      "text": {
        "content": "Found the puppy when I was jogging around the neighbourhood.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "I first saw her on Monday night.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "I think she has been abandoned by her mum.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "No sign of the mother at all.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "She needs a good home that would show her the love and care that she needs.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "I would adopt her myself but I already have 3 dogs of my own.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
