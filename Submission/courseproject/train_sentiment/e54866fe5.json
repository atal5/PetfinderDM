{
  "sentences": [
    {
      "text": {
        "content": "I've been feeding Oreo's mum and his other siblings for the past 2 months.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "They are currently taking a shelter inside the temple behind my office.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Please open your heart to adopt Oreo.He deserves to be loved and cared for.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    },
    {
      "text": {
        "content": "No caging or chaining is allowed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "He must be allowed to run freely within the house compound.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    },
    {
      "text": {
        "content": "Only serious adopters will be entertained.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    }
