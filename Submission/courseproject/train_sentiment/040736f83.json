{
  "sentences": [
    {
      "text": {
        "content": "I was separated from my mum and dumped by the roadside with my sister when we were one month old.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "We were scared, hungry and thirsty...",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "For several days we tried to follow whoever passed us by in hope that they would take us home but we were shooed away each and every time.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    },
    {
      "text": {
        "content": "No one cared to look at us until we were rescued early in the morning one day.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    }
  ],
