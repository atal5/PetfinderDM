  "sentences": [
    {
      "text": {
        "content": "The kitten was surrendered to me in October by my neighbour whose son found him in a drain.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "The kitten was sickly then but thankfully, I managed to successfully nursed her back to good health.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "She has been booked by my Japanese friend who named her as Momo.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "I'm fostering the kitten until my friend could move into her new house which is being renovated for the time being.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    }
  ],
