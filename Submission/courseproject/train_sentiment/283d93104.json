  "sentences": [
    {
      "text": {
        "content": "Our mother had been dumped by someone when pregnant at a temple in Puchong.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "Gave birth to us and the other 3 siblings.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Recently, our mother and the other 3 siblings have been caught by MPSJ dog catchers and we were lucky to escape by hiding in a drain.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "We are intelligent and obedient girls.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "V hope someone can give us a loving home to start our new life.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "No caging or tying for long hours except for precautionary purposes Commitment to spay her is compulsory Interested to adopt pls call me or Mayo Clinic @ or Mr Hew @",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    }
