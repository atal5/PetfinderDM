{
  "sentences": [
    {
      "text": {
        "content": "Two small kittens, abandoned by their mum inside a hardware store near my place of work.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "Only a few weeks old!",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "They can eat wet food but clearly miss their mother.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "Taco (ginger) is the more outgoing and adventurous one, while Nacho (black and white) is more needy.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "They're currently living in my bathroom and my dog is helping to take care of them.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    }
