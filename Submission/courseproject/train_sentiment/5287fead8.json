  "sentences": [
    {
      "text": {
        "content": "Irish was rescued from a home where he was neglected.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    },
    {
      "text": {
        "content": "His fur was matted, had mange so bad there were open wounds on him.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": -0.7
      }
    },
    {
      "text": {
        "content": "Its been 6 mths since the rescue, irish now has his beautiful long fur back and has been neutered.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "He's a very loving attention seeking dog despite all he has been through, his only fear - vets and hates his ears being cleaned and nails cut.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    }
