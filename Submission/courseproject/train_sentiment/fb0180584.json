{
  "sentences": [
    {
      "text": {
        "content": "Max is one of the 7 pups we fostered when his mom gave birth 5 mths ago.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "We have decided to keep one to keep our toy poodle company and we picked Max.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "The rest have been adopted.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Max has been rough on our toy poodle and she dislikes the rough way he treats her, hence we have decided to put him up for adoption.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    }
  ],
