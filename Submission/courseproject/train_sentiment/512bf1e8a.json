{
  "sentences": [
    {
      "text": {
        "content": "Sparky has to find a new home because his owner can't care for him due to personal reasons.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "He has been housed by a vet and has been a blood donor who has saved many lives.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    },
    {
      "text": {
        "content": "He is an active sweet and playful dog and we hope to find him a good forever home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Only for dog lovers who have ample home space for him to run around.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "Please SMS or email me to introduce yourself and experience with dogs and i will get back to you.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
