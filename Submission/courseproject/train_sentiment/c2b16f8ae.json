  "sentences": [
    {
      "text": {
        "content": "Smokey is a handsome fella adopted from his former owner Ms Mills about a year ago.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "And since our circumstances have changed recently, we are now looking for an exceptional good home for Smokey.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Although he is not really a lap cat, but does enjoy being near you and enjoys being petted and stroked at night time or after a nap in the afternoon whenever he feels comfortable with it.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "To find out more about Smokey, you can also visit this link:",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    }
