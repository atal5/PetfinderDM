  "sentences": [
    {
      "text": {
        "content": "The kittens were found around our hostel and motherless.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "Despite being healthy due to the given food by students, we are very scared of how the survival of these kittens as there will be high chances of getting them being dump at the market as reffered to our wardens.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "Please help us since we are able to take care of them since we are living in the hostel.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "We will give the litter and some food that we already bought with no extra charge.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "Please help us and help them.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Please do whatsapp/IM my number instead of calling and messaging.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
