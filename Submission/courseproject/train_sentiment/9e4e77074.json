  "sentences": [
    {
      "text": {
        "content": "Odie was found together with his siter when they were about 3 weeks old in the garbage collection area in our condo.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": -0.9
      }
    },
    {
      "text": {
        "content": "He is about a 5 weeks old now and is eating solid food.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "Not neutered/spayed or vaccinated as he's too young.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": -0.1
      }
    },
    {
      "text": {
        "content": "He is curious, goofy, playful and affectionate and looking for a loving home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "While there is no adoption fee, the potential adopter must agree to spay and vaccinate him..",
        "beginOffset": -1
      },
