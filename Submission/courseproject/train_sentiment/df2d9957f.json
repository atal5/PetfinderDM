  "sentences": [
    {
      "text": {
        "content": "Lucky is extremely good natured, great as a family dog.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "His front right paw has a small birth deformity that gives him a slight hobble when he walks but it doesn't affect his mobility.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Smaller than the average golden and very good boy.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "Owner has to give him up due to personal reasons.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Neutering cost to be borne by adopter.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "Please help find this boy a good owner.",
        "beginOffset": -1
      },
