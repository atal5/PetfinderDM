  "sentences": [
    {
      "text": {
        "content": "George is a beautiful and playful kitty.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "We found him in the street in very bad condition, but after a few weeks of dedicated care and attention he's doing great.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "In the photos one of his eyes looks weird, but now they're both perfect.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "He's to young for spayed/neutered, but I'll be happy to sponsor the treatments.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "We are in Malaysia for vacations and will be living the country in April, so it's URGENT to find him a home within March.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": -0.7
      }
    }
