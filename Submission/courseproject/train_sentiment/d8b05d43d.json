{
  "sentences": [
    {
      "text": {
        "content": "Dexter was rescued by my friend in Puchong.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "He is a very good natured and steady puppy.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Great as a pet and companion.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Intelligent and alert too.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Vaccination and neutering are compulsory, cost to be borne by adopter.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "Please give this loving boy a good home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    }
