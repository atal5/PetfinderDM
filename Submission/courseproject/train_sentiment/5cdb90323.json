  "sentences": [
    {
      "text": {
        "content": "Max was found abandoned with a tvt cancerous tumour, he has undergone chemotherapy and no longer needs any more injections.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": -0.8
      }
    },
    {
      "text": {
        "content": "He has no where to go.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "He needs a free roaming home with a garden and a loving patient family.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "He was returned by previous owner because he is not a young dog and he cries.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "He needs love, we don't know his story before we found him, he could have always been homeless or he could have been dumped, either is cause to cry, please help Max.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    }
