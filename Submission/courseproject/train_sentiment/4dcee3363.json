{
  "sentences": [
    {
      "text": {
        "content": "After a long night of rain, 2 kittens were seen wandering behind the back lane of my office this morning.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "No sight of their mother and we were dirty and hungry.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": -0.9
      }
    },
    {
      "text": {
        "content": "I have brought them back to my office, cleaned them up and fed them.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    },
    {
      "text": {
        "content": "They are healthy and energetic and are ready to be adopted and given a place called home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Please give them a chance and contact me if you are interested.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Thank you.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
  ],
