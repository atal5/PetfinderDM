  "sentences": [
    {
      "text": {
        "content": "Zorro is a healthy playful 3 month old puppy looking for a loving home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Please call Bella or Helena if you are interested.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "Since the puppies were born my friends and I have kept them and there mum safe, fed and played with them 3 times a day.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    },
    {
      "text": {
        "content": "When they were old enough they were vaccinated.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "They are too young to be neutered yet, but we will also take care of that or pay for the cost.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
