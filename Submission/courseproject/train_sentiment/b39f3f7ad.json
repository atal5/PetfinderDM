  "sentences": [
    {
      "text": {
        "content": "Dottie is a pure Dalmatian and was named by my 6 yr old son on account of her dots.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "I adopted her 1-2 months ago but unfortunately she doesnt get along with my other Labrador and end up fighting a lot.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "Dottie is very healthy and active but is deaf.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    },
    {
      "text": {
        "content": "She is not a guard dog (although she does bark at strangers outside the gate) but is suitable for someone looking for an extremely loving dog with good temparament and is good around children.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    }
