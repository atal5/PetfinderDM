{
  "sentences": [
    {
      "text": {
        "content": "This kitty was stuck in the drain for 2 days before I came to rescue.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "I cant keep it as I am allergic to cats fur.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "The kitty doesn't meow much, love to stick around with people, enjoy being pat on the head.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    },
    {
      "text": {
        "content": "I'm urgently seeking for someone to foster this kitty as I will be leaving Malacca next week.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Please contact me if you are interested, thank you very much!",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    }
