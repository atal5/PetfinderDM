{
  "sentences": [
    {
      "text": {
        "content": "I have been feeding Chloe's mum and her other sibling for the past few months.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "They are currently taking a shelter inside the temple behind my office.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Please open your heart to adopt Chloe.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "She deserves to be loved and cared for.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "Future adopter must spay her when she is 5 months old (we will come and check).No caging or chaining is allowed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "She must be allowed to run freely within the house compound.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "Only serious adopters will be entertained.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    }
