  "sentences": [
    {
      "text": {
        "content": "Apollo was abandoned at the Shelter 2 months ago with his eyes still closed and without his mother.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "It was a uphill battle to raise him on bottle feed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "He is absolutely adorable and people friendly.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "A cute little pie.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "Please open your heart and home to him.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Call me at to adopt the little prince",
        "beginOffset": -1
      },
